{
  "name": "AnimeLab PiP Unblocker",
  "version": "0.1",
  "description": "Block PiP events for AnimeLab.",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["https://*.animelab.com/*"],
    "js": ["contentScript.js"],
    "all_frames": true,
    "run_at": "document_start"
  }],
  "icons": { 
    "128": "icon128.png" 
  }
}